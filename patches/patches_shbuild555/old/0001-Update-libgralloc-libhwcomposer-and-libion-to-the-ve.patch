+#ifdef PRIOR_SECURE_CMA_HEAP
+    if (heap_mask & ION_HEAP_TYPE_SECURE_MASK) {
+        data.heap_id_mask = ION_HEAP_TYPE_SECURE2_MASK;
+        ret = ion_ioctl(fd, ION_IOC_ALLOC, &data);
