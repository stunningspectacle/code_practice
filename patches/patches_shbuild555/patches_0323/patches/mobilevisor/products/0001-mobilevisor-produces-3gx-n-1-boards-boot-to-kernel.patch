new file mode 100644
index 0000000..5134312
--- /dev/null
+++ b/sofia3gx/configs/module_boot.xml
@@ -0,0 +1,34 @@
+    <module name="boot_service" enabled="1">
+      <params>
+        <param type="uint32" value="0x00800000" description="kernel_address"/>
+        <param type="uint32" value="0x0"        description="kernel_size, used in standalone vmm only"/>
+        <param type="uint32" value="0x600010"   description="dtb_address"/>
+        <param type="uint32" value="0x0"        description="dtb_size, used in standalone vmm only"/>
+        <param type="uint32" value="0x007ff800" description="setup_sector_address"/>
+        <param type="uint32" value="0x300000"   description="ramdisk_address"/>
+        <param type="uint32" value="0x0"        description="ramdisk_size, used in standalone vmm only"/>
+        <param type="uint32" value="0x0"        description="boot image address, used in standalone vmm only"/>
+        <param type="e820"   description="e820 entries">
+        <!-- E820 entries MAX: 5 -->
+        <!-- possible types: -->
+        <!--    E820_RAM        1
+                E820_RESERVED   2
+                E820_ACPI       3
+                E820_NVS        4
+                E820_UNUSABLE   5 -->
+          <e820 addr=to_string(RAM_CONFIG_LINUX_PSTART) size=to_string(RAM_CONFIG_LINUX_SIZE) type="1" />
+          <e820 addr="0x1BC00000" size="0x04400000" type="2" />
+#ifdef RAM_CONFIG_LINUX_HIGH_PSTART
+          <!-- Anything above 1GB is expected to be detected and configured at runtime for garnet variant -->
+          <e820 addr=to_string(RAM_CONFIG_LINUX_HIGH_PSTART) size="0x20000000" type="1" />
+#endif
+        </param>
+        <param type="uint32" value="1"          description="proxy certificate to secvm enable/disable"/>
+        <param type="uint32" value="0x1CB00000" description="proxy certificate to secvm address"/>
+        <param type="uint32" value="1"          description="proxy secpack to secvm enable/disable"/>
+        <param type="uint32" value="0x1CB01400" description="proxy secpack to secvm address"/>
+        <param type="uint32" value="1"          description="proxy keys to secvm address"/>
+        <param type="uint32" value="0x1CB02400" description="proxy keys to secvm address"/>
+        <param type="uint32" value="1"          description="proxy memory type and memory size from bootloader"/>
+      </params>
+    </module>
