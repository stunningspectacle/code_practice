+  U8 clear_wake_src[2]	  = {0x31, 0xFF}; // WAKESRC_REG, write "1" to clear
+  U8 pmu_off_cmd[2]		  = {0x2A, 0x1};  //PMIC_CHIPCNTRL_REG offset, value
