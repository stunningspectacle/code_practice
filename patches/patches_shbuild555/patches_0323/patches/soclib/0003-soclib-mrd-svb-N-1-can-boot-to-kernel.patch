+  pmu_reg.vprog2sel_reg.VPROG2VSEL_REG_Content = PMIC_Get(PMIC_VPROG2VSEL_REG,(HWREG_U8(*)(HWREG_U8))getPmic_VPROG2VSEL_REG);
