+    /* send data over i2c bus */
+    i2c_bus_write(pmu_i2c_write.bus_id,pmu_i2c_write.slave_address,pmu_i2c_write.i2c_write_buf,pmu_i2c_write.i2c_write_size);
+  }
+
+  return true;
