mkcd() { mkdir -p "$1"; cd "$1"; }
cls() { cd "$1"; ls; }
mkscript() { 
    cmd=
    case "$1" in
        *.sh) cmd="#!/bin/bash" ;;
        *.py) cmd="#!/usr/bin/python" ;;
        *) cmd="unkown"
    esac
    if [ $cmd == "unkown" ]; then
        echo "Not support .$(echo $1 | cut -d. -f2) file"
    else
        touch "$1"
        echo $cmd > "$1"
        chmod +x "$1"
        vim "$1"
    fi
}
