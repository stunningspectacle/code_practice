installAll() {
    [ $# != 1 ] && {
        echo "usage: installAll path"
        return
    }
    PWD=$(pwd)
    cd $1 || return
    for i in $(ls *.apk); do
        echo "Installing $i..."
        adb wait-for-device && adb install $i;
    done
    cd $PWD
}
and() {
    DIR=$(pwd)
    while true
    do
        if [ -d ".repo" ]; then
            source build/envsetup.sh
            break
