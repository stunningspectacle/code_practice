                echo "Not in an Android repo tree."
                break
            fi
            cd ..
        fi
    done
    cd $DIR
}
cscope-android() {
    DIR=$(pwd)
    find $DIR/libcore $DIR/frameworks $DIR/hardware $DIR/packages $DIR/vendor \
    -type l -prune -o \
    -name "*.[chxsS]" -print -o \
    -name "*.cpp" -print -o \
    -name "*.java" -print > $DIR/cscope.files

    cscope -bq
    #cscope -Rbq
}
