vit() {
    if [ -e "$1" ]; then
            vim $1 
            exit
    fi

    files=()
    files_name=`find . -name "$1" | sed -e "/.*.o$/d"`

    i=0
    for file in $files_name
    do
            files[$i]=$file
            echo "$i) $file"
            #let "i = $i + 1"
            i=`expr $i + 1`
    done

    if [ $i -eq 1 ]; then
            vim ${files[0]}
    elif [ $i -eq 0 ]; then
            echo "Can't find file: $1!"
	    return
    else
            echo -n "which one: "
            read a
            vim ${files[$a]} 
    fi
}
